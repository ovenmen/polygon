---
import Users from "components/users";
import MainLayout from "layouts/mainLayout.astro";

const accessToken = Astro.cookies.get('accessToken').value;

if (!accessToken) {
   return Astro.redirect('/admin/sigin');
}
---

<MainLayout title="Polygon Admin CMS | Articles">
    <h1 class="text-3xl text-center my-5">Users</h1>
    <Users client:load />
</MainLayout>