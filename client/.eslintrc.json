{
    "extends": [
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended",
        "react-app"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "eol-last": ["error", "always"]
    }
}
